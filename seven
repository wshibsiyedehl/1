    game.Players.LocalPlayer.Character.Humanoid.Health=Value

    end

    })Tab:AddToggle({

	Name = "无限跳",

	Default = false,

	Callback = function(Value)

		Jump = Value

        game.UserInputService.JumpRequest:Connect(function()

            if Jump then

                game.Players.LocalPlayer.Character.Humanoid:ChangeState("Jumping")

            end

        end)

	end

    })Tab:AddTextbox({

